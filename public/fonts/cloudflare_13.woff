     const acceptedLocales = [
      'en-au',
      'en-ca',
      'en-gb',
      'en-in',
      'en-us',
      'de-de',
      'es-es',
      'fr-fr',
      'it-it',
      'ja-jp',
      'ko-kr',
      'pt-br',
      'zh-tw',
      'zh-cn',
      'zh-Hans-CN',
